<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Shelly">


    <!--SectionRed-->
    <Color x:Key="SectionRed">#E98989</Color>
    <SolidColorBrush x:Key="SectionRedBrush" Color="{StaticResource SectionRed}"></SolidColorBrush>


    <!--SectionBlue-->
    <Color x:Key="SectionBlue">#82CDF2</Color>
    <SolidColorBrush x:Key="SectionBlueBrush" Color="{StaticResource SectionBlue}"></SolidColorBrush>


    <!--SectionPurple-->
    <Color x:Key="SectionPurple">#DF87F5</Color>
    <SolidColorBrush x:Key="SectionPurpleBrush" Color="{StaticResource SectionPurple}"></SolidColorBrush>


    <!--SectionGreen-->
    <Color x:Key="SectionGreen">#FF9F0A</Color>
    <SolidColorBrush x:Key="SectionGreenBrush" Color="{StaticResource SectionGreen}"></SolidColorBrush>


    <!--SectionYellow-->
    <Color x:Key="SectionYellow">#F5E787</Color>
    <SolidColorBrush x:Key="SectionYellowBrush" Color="{StaticResource SectionYellow}"></SolidColorBrush>


    <!--SectionOrange-->
    <Color x:Key="SectionOrange">#F5B787</Color>
    <SolidColorBrush x:Key="SectionOrangeBrush" Color="{StaticResource SectionOrange}"></SolidColorBrush>


</ResourceDictionary>